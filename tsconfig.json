{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@pages/*": ["pages/*"],
      "@hooks/*": ["hooks/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"]
    },
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true
  },
  "include": ["src"]
}
